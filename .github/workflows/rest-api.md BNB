  <span class="pl-s">"clientOrderId"</span>: <span class="pl-s">"cancelMyOrder1"</span><span class="pl-kos">,</span>
  <span class="pl-s">"price"</span>: <span class="pl-s">"2.00000000"</span><span class="pl-kos">,</span>
