    <span class="pl-s">"clientOrderId"</span>: <span class="pl-s">"myOrder1"</span><span class="pl-kos">,</span>
    <span class="pl-s">"price"</span>: <span class="pl-s">"0.1"</span><span class="pl-kos">,</span>
    <span class="pl-s">"origQty"</span>: <span class="pl-s">"1.0"</span><span class="pl-kos">,</span>
